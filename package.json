{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "description": "My supreme Nuxt.js project",
  "author": "",
  "private": true,
  "scripts": {
    "article:add": "node commands/addArticle.js",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern 'static/' .",
    "precommit": "npm run lint",
    "test": "jest",
    "clear:json": "rimraf static/posts/_json && mkdirp static/posts/_json",
    "md-blogify": "md-blogify --previewLength 140 --summaryPath static/posts/_json/summary.json static/posts static/posts/_json",
    "build:md": "npm run clear:json && npm run md-blogify",
    "release": "npm run build:md && npm run generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.9.4",
    "@nuxtjs/style-resources": "^0.1.2",
    "cross-env": "^5.2.0",
    "github-markdown-css": "^3.0.1",
    "lodash": "^4.17.11",
    "md-blogify": "git+https://github.com/ryou/md-blogify.git",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-fs-extra": "^0.8.2",
    "nuxt": "^2.11.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.5"
  },
  "engines": {
    "node": "10.16.2",
    "npm": "6.9.0"
  }
}
